{
  "name": "activew",
  "version": "1.0.0",
  "description": "Application environment in Wiki",
  "main": ".storybook/config.js",
  "dependencies": {
    "lodash": "4.17.4",
    "remark": "6.2.0",
    "mdast-util-to-hast": "2.2.0",
    "hast-to-hyperscript": "2.0.4",
    "yjs": "12.1.4",
    "y-memory": "8.0.7",
    "y-websockets-server": "9.1.5",
    "minimist": "1.2.0",
    "socket.io": "1.7.2",
    "y-websockets-client": "8.0.15",
    "y-array": "10.0.6",
    "y-text": "9.2.0",
    "react": "15.4.2",
    "react-dom":  "15.4.2",
    "react-tap-event-plugin": "2.0.1",
    "react-markdown": "2.4.4",
    "brace": "0.9.1",
    "react-ace": "4.1.1",
    "react-monaco-editor": "0.7.3"
  },
  "devDependencies": {
    "cross-env": "3.1.4",
    "npm-run-all": "4.0.1",
    "rimraf": "2.5.4",
    "babel-cli": "6.22.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "@kadira/storybook": "2.35.3",
    "eslint": "3.14.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-ava": "4.0.1",
    "eslint-config-airbnb": "14.0.0",
    "ava": "0.17.0",
    "nyc": "10.1.2",
    "babel-plugin-istanbul": "3.1.2",
    "esdoc": "0.5.2",
    "webpack": "1.14.0",
    "babel-loader": "6.2.10",
    "eslint-loader": "1.6.1",
    "json-loader": "0.5.4",
    "copy-webpack-plugin": "4.0.1",
    "webpack-dev-server": "1.16.3",
    "http-server": "0.9.0"
  },
  "scripts": {
    "clean": "rimraf build storybook-static .nyc_output coverage esdoc",
    "lint": "eslint --color src/**/*.js src/**/*.jsx .storybook/**/*.js stories/**/*.js stories/**/*.jsx webpack.*.js",
    "build": "run-p -l build:*",
    "build:app": "cross-env NODE_ENV=production webpack --progress --colors",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:storybook": "cross-env NODE_ENV=production build-storybook",
    "storybook": "cross-env NODE_ENV=development run-p -l server:storybook server:websocket",
    "start": "cross-env NODE_ENV=development run-p -l server:devserver server:websocket",
    "server": "cross-env NODE_ENV=production run-p -l server:static server:websocket",
    "server:storybook": "start-storybook -p 6006",
    "server:devserver": "webpack-dev-server --progress --colors",
    "server:websocket": "babel-node server-websocket.js",
    "server:static": "http-server",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc --extension .jsx --reporter lcov --reporter text npm test",
    "watch:ava": "npm run test:ava -- --watch"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Ohtake, Tomohiro <ohtake.tomohiro@jp.fujitsu.com>",
  "contributors": [
    "Seki, Yohei <seki.yohei@jp.fujitsu.com>"
  ],
  "license": "MIT"
}
