{
  "name": "cattaz",
  "version": "1.0.0",
  "description": "Application environment in Wiki",
  "main": ".storybook/config.js",
  "dependencies": {
    "@storybook/react": "3.1.6",
    "babel-polyfill": "^6.26.0",
    "brace": "^0.10.0",
    "codemirror": "^5.30.0",
    "diff": "^3.4.0",
    "finalhandler": "^1.1.0",
    "github-markdown-css": "^2.9.0",
    "hast-to-hyperscript": "^3.0.2",
    "lodash": "^4.17.4",
    "mdast-util-to-hast": "^2.5.0",
    "minimist": "^1.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-ace": "^5.4.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-split-pane": "^0.1.66",
    "remark": "^8.0.0",
    "router": "^1.3.2",
    "socket.io": "^2.0.3",
    "verge": "^1.10.2",
    "whatwg-fetch": "^2.0.3",
    "y-array": "^10.1.4",
    "y-memory": "^8.0.9",
    "y-text": "^9.5.1",
    "y-websockets-client": "^8.0.16",
    "y-websockets-server": "^9.2.1",
    "yjs": "^12.3.3"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "esdoc": "^1.0.3",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "http-server": "^0.10.0",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "source-map-explorer": "^1.5.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2"
  },
  "scripts": {
    "clean": "rimraf build storybook-static .nyc_output coverage esdoc licenses.txt",
    "lint": "eslint --color src/**/*.js src/**/*.jsx test/**/*.js .storybook/**/*.js stories/**/*.js stories/**/*.jsx *.js",
    "build": "npm-run-all -l -p build:app -p build:esdoc build:licenses build:sme build:storybook",
    "build:app": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.babel.js",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:licenses": "yarn licenses generate-disclaimer --prod > licenses.txt",
    "build:sme": "source-map-explorer build/bundle.js build/bundle.js.map --html > build/sme.html",
    "build:storybook": "cross-env NODE_ENV=production build-storybook",
    "storybook": "cross-env NODE_ENV=development run-p -l server:storybook server:websocket",
    "start": "cross-env NODE_ENV=development run-p -l server:devserver server:websocket",
    "server": "cross-env NODE_ENV=production run-p -l server:static server:websocket",
    "server:storybook": "start-storybook -p 6006",
    "server:devserver": "webpack-dev-server --progress --colors --config webpack.config.babel.js",
    "server:websocket": "babel-node server-websocket.js",
    "server:static": "http-server",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc --extension .jsx --reporter lcov --reporter text yarn test",
    "watch:ava": "yarn run test:ava -- --watch"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Ohtake, Tomohiro <ohtake.tomohiro@jp.fujitsu.com>",
  "contributors": [
    "Seki, Yohei <seki.yohei@jp.fujitsu.com>"
  ],
  "license": "MIT"
}
