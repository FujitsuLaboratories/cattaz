{
  "name": "cattaz",
  "version": "1.0.0",
  "description": "Application environment in Wiki",
  "main": ".storybook/config.js",
  "dependencies": {
    "brace": "^0.10.0",
    "hast-to-hyperscript": "^2.0.4",
    "lodash": "^4.17.4",
    "mdast-util-to-hast": "^2.3.0",
    "minimist": "^1.2.0",
    "react": "^15.4.2",
    "react-ace": "^4.1.6",
    "react-dom": "^15.4.2",
    "react-markdown": "^2.4.4",
    "react-monaco-editor": "^0.7.3",
    "react-tap-event-plugin": "^2.0.1",
    "remark": "^6.2.0",
    "socket.io": "^1.7.3",
    "y-array": "^10.0.6",
    "y-memory": "^8.0.8",
    "y-text": "^9.2.2",
    "y-websockets-client": "^8.0.15",
    "y-websockets-server": "^9.1.6",
    "yjs": "^12.1.4"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "ava": "^0.18.2",
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "esdoc": "^0.5.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.1.2",
    "rimraf": "^2.6.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "scripts": {
    "clean": "rimraf build storybook-static .nyc_output coverage esdoc",
    "lint": "eslint --color src/**/*.js src/**/*.jsx .storybook/**/*.js stories/**/*.js stories/**/*.jsx webpack.*.js",
    "build": "run-p -l build:*",
    "build:app": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.babel.js",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:storybook": "cross-env NODE_ENV=production build-storybook",
    "storybook": "cross-env NODE_ENV=development run-p -l server:storybook server:websocket",
    "start": "cross-env NODE_ENV=development run-p -l server:devserver server:websocket",
    "server": "cross-env NODE_ENV=production run-p -l server:static server:websocket",
    "server:storybook": "start-storybook -p 6006",
    "server:devserver": "webpack-dev-server --progress --colors --config webpack.config.babel.js",
    "server:websocket": "babel-node server-websocket.js",
    "server:static": "http-server",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc --extension .jsx --reporter lcov --reporter text yarn test",
    "watch:ava": "yarn run test:ava -- --watch"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Ohtake, Tomohiro <ohtake.tomohiro@jp.fujitsu.com>",
  "contributors": [
    "Seki, Yohei <seki.yohei@jp.fujitsu.com>"
  ],
  "license": "MIT"
}
