    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "scripts": {
    "clean": "rimraf build storybook-static .nyc_output coverage esdoc",
    "lint": "run-p -l lint:*",
    "lint:eslint": "eslint --color --report-unused-disable-directives src/**/*.js src/**/*.jsx test/**/*.js .storybook/**/*.js stories/**/*.jsx *.js",
    "lint:markdownlint": "markdownlint --config .markdownlint.json README.md docs",
    "build": "npm-run-all -l -p build:app -p build:esdoc build:licenses build:sme build:storybook",
    "build:app": "cross-env NODE_ENV=production webpack --mode production --progress --colors --config webpack.config.babel.js",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:licenses": "yarn licenses generate-disclaimer --prod > build/licenses.txt",
    "build:sme": "source-map-explorer build/bundle.js build/bundle.js.map --html > build/sme.html",
    "build:storybook": "cross-env NODE_ENV=production build-storybook",
    "storybook": "cross-env NODE_ENV=development SERVER_MODE=storybook run-p -l server:storybook server:dev",
    "landingpage": "cross-env NODE_ENV=production SERVER_MODE=landingpage babel-node server.js",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node server.js",
    "server": "cross-env NODE_ENV=production babel-node server.js",
    "server:storybook": "start-storybook -p 6006",
    "server:dev": "nodemon --exec babel-node server.js",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/*.ava.js test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc --extension .jsx --reporter lcov --reporter text npm run test",
    "watch:ava": "yarn run test:ava -- --watch"
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "./test/setup-enzyme.js"
    ]
  },
  "author": "Ohtake, Tomohiro <ohtake.tomohiro@jp.fujitsu.com>",
  "contributors": [
    "Seki, Yohei <seki.yohei@jp.fujitsu.com>"
  ],
  "license": "MIT"
}
