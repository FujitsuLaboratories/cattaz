{
  "name": "activew",
  "version": "1.0.0",
  "description": "Application environment in Wiki",
  "main": ".storybook/config.js",
  "dependencies": {
    "lodash": "4.16.3",
    "remark": "6.0.1",
    "hast-util-sanitize": "1.1.0",
    "remark-react": "3.0.2",
    "react": "15.3.2",
    "react-dom":  "15.3.2",
    "react-tap-event-plugin": "1.0.0",
    "react-markdown": "2.4.2",
    "react-monaco-editor": "0.5.0"
  },
  "devDependencies": {
    "cross-env": "3.0.0",
    "npm-run-all": "3.1.0",
    "rimraf": "2.5.4",
    "babel-cli": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "@kadira/storybook": "2.20.1",
    "eslint": "3.7.1",
    "eslint-plugin-import": "2.0.0",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-ava": "3.1.0",
    "eslint-config-airbnb": "12.0.0",
    "ava": "0.16.0",
    "webpack": "1.13.2",
    "babel-loader": "6.2.5",
    "eslint-loader": "1.5.0",
    "json-loader": "0.5.4",
    "copy-webpack-plugin": "3.0.1",
    "webpack-dev-server": "1.16.1",
    "http-server": "0.9.0"
  },
  "scripts": {
    "clean": "rimraf build storybook-static",
    "lint": "eslint --color src/**/*.js src/**/*.jsx .storybook/**/*.js stories/**/*.js stories/**/*.jsx webpack.*.js",
    "build": "run-p -l build:*",
    "build:app": "cross-env NODE_ENV=production webpack --progress --colors",
    "build:storybook": "cross-env NODE_ENV=production build-storybook",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
    "server": "http-server",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "watch:ava": "npm run test:ava -- --watch"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "",
  "license": "UNLICENSED"
}
