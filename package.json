{
  "name": "activew",
  "version": "1.0.0",
  "description": "Application environment in Wiki",
  "main": ".storybook/config.js",
  "dependencies": {
    "lodash": "4.17.2",
    "remark": "6.2.0",
    "mdast-util-to-hast": "2.1.1",
    "hast-to-hyperscript": "2.0.4",
    "react": "15.4.1",
    "react-dom":  "15.4.1",
    "react-tap-event-plugin": "2.0.1",
    "react-markdown": "2.4.2",
    "react-monaco-editor": "0.7.0"
  },
  "devDependencies": {
    "cross-env": "3.1.3",
    "npm-run-all": "3.1.2",
    "rimraf": "2.5.4",
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "@kadira/storybook": "2.33.1",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-react": "6.4.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-ava": "4.0.0",
    "eslint-config-airbnb": "12.0.0",
    "ava": "0.17.0",
    "nyc": "10.0.0",
    "babel-plugin-istanbul": "3.0.0",
    "esdoc": "0.4.8",
    "webpack": "1.13.3",
    "babel-loader": "6.2.8",
    "eslint-loader": "1.6.1",
    "json-loader": "0.5.4",
    "copy-webpack-plugin": "4.0.1",
    "webpack-dev-server": "1.16.2",
    "http-server": "0.9.0"
  },
  "scripts": {
    "clean": "rimraf build storybook-static .nyc_output coverage esdoc",
    "lint": "eslint --color src/**/*.js src/**/*.jsx .storybook/**/*.js stories/**/*.js stories/**/*.jsx webpack.*.js",
    "build": "run-p -l build:*",
    "build:app": "cross-env NODE_ENV=production webpack --progress --colors",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:storybook": "cross-env NODE_ENV=production build-storybook",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
    "server": "http-server",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc --extension .jsx --reporter lcov --reporter text npm test",
    "watch:ava": "npm run test:ava -- --watch"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "",
  "license": "UNLICENSED"
}
